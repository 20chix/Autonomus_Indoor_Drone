<robot name="milk"
		   xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor"
       xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"
       xmlns:interface="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface">
	<link name ="milk">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.75"/>
			<inertia ixx="0.002689" ixy="0" ixz="0" iyy="0.002508" iyz="0" izz="0.000197"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://cvg_sim_gazebo/Media/models/milk.dae"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.009"/>
			<geometry>
        <box size="0.110 0.055 0.2"/> <!-- taken from blender model -->
			</geometry>
		</collision>
	</link>
</robot>
